{
  "name": "roblox obby",
  "tree": {
    "$className": "DataModel",

    "ReplicatedStorage": {
      "Shared": {
        "$path": "src/shared"
      },
      "Remotes": {
        "$className": "Folder",
        "UseItem": {
          "$className": "RemoteEvent"
        },
        "UpdateScore": {
          "$className": "RemoteEvent"
        },
        "StageComplete": {
          "$className": "RemoteEvent"
        },
        "StartGame": {
          "$className": "RemoteEvent"
        },
        "UpdateLeaderboard": {
          "$className": "RemoteEvent"
        },
        "PlayerDied": {
          "$className": "RemoteEvent"
        }
      },
      "StageTemplates": {
        "$className": "Folder"
      }
    },

    "ServerScriptService": {
      "Server": {
        "$path": "src/server"
      }
    },

    "StarterPlayer": {
      "StarterPlayerScripts": {
        "Client": {
          "$path": "src/client"
        }
      }
    },

    "Workspace": {
      "SpawnLocation": {
        "$className": "SpawnLocation",
        "$properties": {
          "Name": "LobbySpawn",
          "Anchored": true,
          "Position": [0, 1, 0],
          "Size": [8, 1, 8],
          "Neutral": true,
          "Color": [0.3, 0.8, 0.3],
          "Material": "Neon"
        }
      },
      "Lobby": {
        "$className": "Folder",
        "Floor": {
          "$className": "Part",
          "$properties": {
            "Anchored": true,
            "Color": [0.3, 0.5, 0.3],
            "Material": "Grass",
            "Size": [100, 2, 100],
            "Position": [0, -1, 0],
            "CustomPhysicalProperties": [0.7, 2.0, 0.1, 1.0, 0.5]
          }
        },
        "StartPortal": {
          "$className": "Part",
          "$properties": {
            "Name": "StartPortal",
            "Anchored": true,
            "Color": [0.0, 0.8, 0.4],
            "Material": "Neon",
            "Size": [10, 15, 2],
            "Position": [0, 7.5, 40],
            "Transparency": 0.3,
            "CanCollide": false
          }
        }
      },
      "Stages": {
        "$className": "Folder"
      },
      "KillBrick": {
        "$className": "Part",
        "$properties": {
          "Anchored": true,
          "Color": [0.7, 0.1, 0.1],
          "Transparency": 0.8,
          "Size": [2000, 1, 2000],
          "Position": [0, -25, 500],
          "CanCollide": false
        }
      }
    },

    "Lighting": {
      "$properties": {
        "Ambient": [0.2, 0.2, 0.25],
        "Brightness": 2,
        "GlobalShadows": true,
        "Outlines": false,
        "Technology": "Voxel",
        "ClockTime": 14
      }
    },

    "SoundService": {
      "$properties": {
        "RespectFilteringEnabled": true
      }
    },

    "StarterGui": {
      "GameUI": {
        "$className": "ScreenGui",
        "$properties": {
          "ResetOnSpawn": false,
          "IgnoreGuiInset": false
        }
      }
    }
  }
}
